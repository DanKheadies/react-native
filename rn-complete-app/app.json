{
  "expo": {
    "name": "ARN Complete App",
    "description": "An app showcasing the lessons and learning from Academind's React Native - The Practical Guide [2020 Edition] course with Maximilian Schwarzmuller via Udemy.com.",
    "slug": "arn-complete-app",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/places.png",
    "splash": {
      "image": "./assets/splash_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#171717"
    },
    "updates": {
      "fallbackToCacheTimeout": 1000
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android" : {
      "package": "com.holisticgaming.arn_complete_app",
      "versionCode": 1,
      "adaptiveIcon": {
        "backgroundColor": "#171717",
        "foregroundImage": "./assets/places-adaptive.png"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "_REMOVED_"
        }
      }
    },
    "ios": {
      "bundleIdentifier": "com.holisticgaming.arn-complete-app",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
